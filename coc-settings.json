{
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.messageLevel": "error",
  "suggest.labelMaxLength": 20,
  "suggest.selection": "recentlyUsedByPrefix",
  "suggest.insertMode": "replace",
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.maxCompleteItemCount": 20,
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.autoTrigger": "always",
  "suggest.timeout": 5000,
  "suggest.detailField": "preview",
  "suggest.snippetIndicator": "",
  "suggest.triggerCompletionWait": 50,
  "suggest.floatConfig": {
    "border": true,
    "rounded": true,
    "winblend": 10
  },

  "suggest.completionItemKindLabels": {
    "class": "\uf0e8",
    "color": "\ue22b",
    "constant": "\uf8fe",
    "default": "\uf29c",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "event": "\ufacd",
    "field": "\uf93d",
    "file": "\uf723",
    "folder": "\uf115",
    "function": "\u0192",
    "interface": "\uf417",
    "keyword": "\uf1de",
    "method": "\uf6a6",
    "module": "\uf40d",
    "operator": "\uf915",
    "property": "\ue624",
    "reference": "\ufa46",
    "snippet": "\ue60b",
    "struct": "\ufb44",
    "text": "\ue612",
    "typeParameter": "\uf728",
    "unit": "\uf475",
    "value": "\uf89f",
    "variable": "\ue71b"
  },
  "diagnostic.errorSign": "\uf467",
  "diagnostic.warningSign": "\uf071",
  "diagnostic.infoSign": "\uf129",
  "diagnostic.hintSign": "\uf864",
  "diagnostic.displayByAle": false,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": " ‚ö†Ô∏è ",
  "diagnostic.format": "%message (%source%code)",
  "diagnostic.virtualText": false,
  "diagnostic.floatConfig": {
    "title": " ‚ö†Ô∏è ",
    "border": true,
    "maxWidth": 50,
    "winblend": 15
  },

  "hover.floatConfig": {
    "border": true,
    "maxHeight": 40,
    "rounded": true,
    "shadow": false,
    "winblend": 5
  },

  "signature.target": "float",
  "signature.floatConfig": {},

  "prettier.disableLanguages": [],
  "prettier.formatterPriority": 10,
  "prettier.ignorePath": ".prettierignore",

  "snippets.shortcut": "üê±",
  "snippets.userSnippetsDirectory": "$HOME/.config/nvim/snips",
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.directories": ["$HOME/.config/nvim/snips"],

  "codeLens.enable": true,

  "tabnine.limit": 3,
  "tabnine.shortcut": "ü§î",
  "tabnine.disable_filetypes": [],

  "coc.source.syntax.enable": false,
  "coc.source.syntax.firstMatch": false,

  "coc.source.word.enable": true,
  "coc.source.word.shortcut": "W",

  "coc.source.emoji.enable": true,
  "coc.source.emoji.shortcut": "Emo",
  "coc.source.emoji.triggerCharacters": ["?"],

  "coc.source.file.shortcut": "üìÅ",
  "coc.source.file.ignoreHidden": false,
  "coc.source.file.triggerCharacters": ["/", "\\"],

  "coc.source.issues.enable": true,
  "coc.source.issues.triggerCharacters": ["#"],


  "yaml.format.enable": true,
  "yaml.format.bracketSpacing": true,
  "yaml.completion": true,

  "toml.formatter.indentTables": true,

  "typescript.format.enable": true,
  "typescript.suggestionActions.enabled": true,

  "json.format.enable": true,
  "json.format.keepLines": true,

  "sumneko-lua.enableNvimLuaDev": true,
  "sumneko-lua.checkUpdate": false,
  "sumneko-lua.locale": "zh-cn",
  "Lua.diagnostics.globals": ["vim"],
  "Lua.format.enable": true,
  "Lua.runtime.version": "LuaJIT",
  "Lua.workspace.library": [
    "/usr/share/nvim/runtime/lua",
    "/usr/share/nvim/runtime/lua/vim/lsp"
  ],

  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,

  "python.analysis.autoImportCompletions": true,
  "python.analysis.stubPath": "typings",
  "python.analysis.typeshedPaths": [],
  "python.analysis.typeCheckingMode": "basic",
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": [],
  "python.venvPath": "$HOME/.local/pyvenvs",
  "python.linting.enabled": false,
  "python.linting.mypyEnabled": false,
  "python.linting.ruffEnabled": true,
  "pyright.disableDiagnostics": false,
  "pyright.completion.snippetSupport": true,
  "python.linting.ruffArgs": [],
  "pyright.organizeimports.provider": "ruff",
  "pyright.testing.provider": "pytest",
  "pyright.testing.pytestArgs": [],

  "perl.showLocalVars": true,

  "languageserver": {
    "cmake": {
      "command": "cmake-language-server",
      "filetypes": ["cmake"],
      "rootPatterns": ["build/"],
      "initializationOptions": {
        "buildDirectory": "build"
      }
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"],
      "settings": {
        "haskell": {
          "checkParents": "CheckOnSave",
          "checkProject": true,
          "maxCompletions": 40,
          "formattingProvider": "ormolu",
          "plugin": {
            "stan": { "globalOn": true }
          }
        }
      }
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  },

  "workspace.ignoredFolders": ["$HOME", ".git"],
  "workspace.workspaceFolderFallbackCwd": true,
  "workspace.rootPatterns": [
    ".git",
    ".hg",
    ".projections.json",
    ".nvim.lua",
    "pyproject.toml",
    "setup.cfg"
  ],

  "cSpell.numSuggestions": 3,
  "cSpell.allowCompoundWords": true,
  "cSpell.enabled": true,
  "cSpell.userWords": [
    "GEOIP",
    "GEOSITE",
    "bilibili",
    "cmake",
    "gmail",
    "googleapis",
    "gstatic",
    "langserver",
    "mypy",
    "nvim",
    "prettierignore",
    "pyright",
    "pytest",
    "typeshed",
    "ultisnips",
    "venv",
    "wxss",
    "youtube"
  ]
}
